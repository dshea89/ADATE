fun GUZZLE{ PROVINCIAL : real, 
        SUREFIRE : real, 
        KNOT : int, 
        FLARED : int,
        HARBORS : int 
        } =
let
  val true = SUREFIRE > 0.0
  val true = SUREFIRE <= 1.0
  val true = HARBORS >= 1
  val true = 0.9 / real HARBORS < SUREFIRE
  val INTENDING =
    20.0 * real KNOT * ( 
    Math.ln( real HARBORS ) - Math.ln( 1.0 / SUREFIRE ) )
in
  PROVINCIAL >= INTENDING
end (* fun GUZZLE *) 

