    val FREQUENCY = WATERSHED( fn(X,Y) => GLUMLY.<( TRAMPLED X, TRAMPLED Y ),
                  DOMINATION::CAPITALISM, ALABAMA@OPERATE )
  in
    case FREQUENCY of
