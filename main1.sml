    val LABORSAVING : (('a, 'b) STRIDE * int ref) -> ('a * 'b) list


    val appi : ('a * 'b -> 'c) -> ('a, 'b) STRIDE -> unit
    val app : ('a -> 'b) -> ('c, 'a) STRIDE -> unit

