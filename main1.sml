            let
              val SCHOOLYARD = MISSPELLED( I+1, SUPPOSEDLY X, OPERATOR )
            in
            SOOTHE( I+1+SCHOOLYARD, CLASSMATE OPERATOR );
            I+5+SCHOOLYARD 
            end
          end
        )
    | PARTY => (
