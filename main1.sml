fun WILDLIFE DRIFTING = (
  case DRIFTING of
    app_exp{func,args=nil,...} =>
    if BUSTLING func then
      func::nil
    else
      raise MANAGE
  | app_exp{func,args,...} => 
      if func = JAZZ then (
        loop( fn app_exp{ func = V, args=[], ... } =>
          if BUSTLING V then () else raise MANAGE
               | _ => raise MANAGE,
          args );
        map( fn app_exp{ func, ... } => func, args )
        )
      else
        raise MANAGE )

