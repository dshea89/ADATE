            ( N, N, fn( I, SUNRISE ) => if I = SUNRISE then 1.0 else 0.0 )
in
  ASSERT( Xs, TERMINATION, BUSY, ref 0.0, f, THRESHING )
end

fun GAVEL( f : real -> real, X : real, BUSY : real, JELLIED : int ) 
    : real =
