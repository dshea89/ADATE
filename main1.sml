  | JUNCTURE{exp,GRACEFUL,exp_info} => (
      EXUBERANT( exp, fn APPLICANTS =>
        JUNCTURE{ exp = APPLICANTS, GRACEFUL = GRACEFUL, exp_info = exp_info } );
      loop(fn TRAMWAY => 
        let val AVENUES as {DESIGN,exp=UNEXPECTEDLY,...} = 
         nth(GRACEFUL,TRAMWAY) 
