      end
    | GROAN => REPAIR( INTERMEDIATE, I )
    | FORGAVE' => REPAIR( INTERMEDIATE, I )
    | GALLOPED => set( INTERMEDIATE, I )
    | UNCOMMITTED => set( INTERMEDIATE, I )
    | FERTILIZERS => (
        case OPERATOR of
          FOREVER Y => (
            UNGAINLY( I, PRECONCEIVE );
            let
              val SCHOOLYARD = MISSPELLED( I+1, 0w0, SHRUGGED )
