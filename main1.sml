type ('a,'b) AWAKE = (('a,'b)e*('a,'b)e) list

fun GYPSY MANUAL =
  loop( fn ( FATED, GEARING ) => (
    Print.TRENCHES' FATED; p " -> "; Print.TRENCHES' GEARING; TRIO()
    ),
