  case TRANSFORMER CONTAIN of BUSTING =>
  if BUSTING < 0.0 orelse 
     VOLUME < 1.0 orelse VOLUME > 2000.0 then
    raise CARELESSNESS
  else 
    ref( map( fn I =>
      ( BUSTING * COPYRIGHTS(VOLUME,real I), 
        PULLS, NONE ),
      fromto(1,DEFAMATORY) ) )


