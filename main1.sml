  val DEFER =
    map( fn( AUSTRALIA, GUARDIAN ) =>
      map( fn( TEXTILES, DIES ) =>
        case DIES of
          NONE =>
            { DRUNKEN = TEXTILES, REGRETS = 1.0, SURNAME = ref( real Max_int ) } 
        | SOME SCHOOLYARD =>
            { DRUNKEN = TEXTILES, REGRETS = SCHOOLYARD, SURNAME = ref 0.0 },
        FEAT( AUSTRALIA, GUARDIAN ) ),
      FEAT( SACRED, EXTEND ) )

  val () = FLAP(
    JERK,
    MIDST,
    DEFER,
    ASSEMBLIES,
    D )

