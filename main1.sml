                      ENDURE(COSTUMES,BREEZY,AGITATION(AVENUES,nil)),
                      filter( fn AVENUES' => not(ATHEISM(AVENUES,AVENUES')), SCOTLAND ) )
              end,
            DUAL(COSTUMES)
            ),
          SCOTLAND
