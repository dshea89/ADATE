exception HUMOROUS
fun DEPARTMENTAL( ty_con_exp( PREACHING, [] ) ) = [PREACHING]
  | DEPARTMENTAL( ty_con_exp( PREACHING, CLAWED ) ) =
      if PREACHING <> MEDICAL then
        raise HUMOROUS
      else
