fun ACCUMULATE( DEFER : real list list ) : real list list =
  case DEFER of
    [] => DEFER
  | [ _ ] => DEFER
  | [ _, _ ] => DEFER
  | Xs1 :: SHAKE :: COCKTAILS :: DEFER =>
  let
    val N = max2( op<,
             length( NEAT'( RECENT, [ Xs1, SHAKE ] ) ),
             length( NEAT'( RECENT, [ SHAKE, COCKTAILS ]  ) ) )

    val L = length SHAKE

