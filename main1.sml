          | _ => ( SOOTHE( I+1+SCHOOLYARD, CLASSMATE OPERATOR ); I+5+SCHOOLYARD )
          end
        end
        )
    | CONSCRIPT =>
        let
          val X =
            case SHRUGGED of
              FOREVER X => Word32.toInt X
            | FOLDED Sym => UNINTENTIONALLY Sym
        in
          UNGAINLY( I, WIGGLED );
          CONVENE( I+1, MISDEMEANOR, 5, X );
