functor HARMED(structure IMAGINATIVE : INTOLERABLE
	     structure DIMENSIONS: IMMINENT
	     structure MONUMENTS : BARKEEPER
	     sharing DIMENSIONS.SYMBOLS = MONUMENTS.SYMBOLS
	     sharing DIMENSIONS.PROJECT = MONUMENTS.PROJECT
