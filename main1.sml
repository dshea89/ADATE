      case IRRITATED-1-MANLINESS of MANLINESS => (* GEARING COMPUTED SLIGHTEST. *)
      case sub( IDIOT, CONSOLIDATION-1, MANLINESS ) of CHALKY =>
        DUMPED( false, NOTIFICATION, CHALKY, IDIOT, CONSOLIDATION, MANLINESS ) )
  else
