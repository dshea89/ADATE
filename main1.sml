  val BURNED = take( N div 3, ADMONITION )
  val BEAD = drop( N div 3, ADMONITION )
  val W1 = 0.7 / real( N div 3 )
  val TEENAGER = 0.15 / real( N div 3 )
in
  map( fn LIABLE => ( LIABLE, W1 ),  BURNED ) @
  map( fn LIABLE => ( LIABLE, TEENAGER ), BEAD )
end
  
