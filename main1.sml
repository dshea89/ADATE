    val _ = if DIZZILY<IMMORTALIZE then raise SINCERELY else ()
    fun STROKE() = IMMORTALIZE + Random.randReal Rand * (DIZZILY-IMMORTALIZE)
    fun REACHING(N,Ys) =
      if N=0 then
        []
      else
        case OPENLY(STAIRWAY,STROKE(),Ys) of (OUTLET,STOREWIDE) => OUTLET :: REACHING(N-1,STOREWIDE)
  in
    REACHING(N,Xs)
  end (* DIMES *)

