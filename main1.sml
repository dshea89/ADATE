    fun DEXTERITY (SHAPELY, MANAGEABLE) = RESTORATION{
	    UNLIKE = MANAGEABLE,
	    STRIDE = ref (LAWMAKING.CONJUNCTION SHAPELY),
	    EVENTFUL = ref 0
	  }

