fun INGROWN( Xs : symbol list, Ys : symbol list ) =
  let
    val TORRENTS : unit AWRY.YELPING = 
      AWRY.DEXTERITY( length Xs, TENS )
    val _ = loop( fn X => AWRY.SIESTA TORRENTS (X,()), Xs )
  in
    filter( fn Y =>
      case AWRY.find TORRENTS Y of
        NONE => false
      | SOME _ => true,
      Ys )
  end

