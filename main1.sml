    case SING of
      NONE => []
    | SOME( _, REMOVE ) =>
        map( fn( BOARDROOM, ELASTIC ) => ( #2 BOARDROOM, ELASTIC ),
          REMOVE )

  val EXTEND = 
    WEDNESDAY( PETITIONER Timer + 
               max(op<, CONFLICTING::AUSTRALIA ) * 
